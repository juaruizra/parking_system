<h1>Locations#INDEX</h1>

<% @locations.each do |aux| %>
  <div>
    Address:  <%= aux.address %>
    Latitude: <%= aux.latitude %>
    Longitude:<%= aux.longitude %>
  </div>
<%end%>

<div class="col-md-2 button-opt">
  <%= link_to "Add Locations",    new_location_path,      class: 'btn btn-success'%>
</div>
